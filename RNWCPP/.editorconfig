# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = false

# Xml project files
[*.{csproj,vcxproj,pssproj,vcxproj.filters}]
indent_size = 2
end_of_line = crlf

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

[*.ps1]
indent_style = tab
indent_size = 4
