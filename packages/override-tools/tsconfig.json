{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es6",
        "module": "commonjs",
        "outDir": "out",
        "sourceMap": true,
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "rootDirs": ["src"],
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
