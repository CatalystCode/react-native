    <Message Text="%0A" />
    <Message Text="=> UnmergedWinmdDirectory  [$(UnmergedWinmdDirectory)]" />
    <Message Text="=> MergedWinmdDirectory    [$(MergedWinmdDirectory)]" />
    <Message Text="=> FollyDir                [$(FollyDir)]" />
    <Message Text="=> ReactNativeWindowsDir   [$(ReactNativeWindowsDir)]" />
