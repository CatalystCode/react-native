<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Microsoft.ProjectReunion.ReactNative</id>
    <version>0.0.1-prerelease</version>
